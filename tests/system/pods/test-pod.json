{
  "id": "/testserver",
  "containers": [
    {
      "name": "healthApp1",
      "exec": {
        "command": {
          "shell": ". /opt/mesosphere/environment.export && ./test_server.py $PORT_HTTPENDPOINT"
        }
      },
      "resources": {
        "cpus": 0.1,
        "mem": 32,
        "disk": 0,
        "gpus": 0
      },
      "endpoints": [
        {
          "name": "httpendpoint",
          "containerPort": null,
          "hostPort": 0,
          "protocol": [
            "tcp"
          ],
          "labels": {}
        }
      ],
      "image": {
        "kind": "docker",
        "id": "debian:jessie"
      },
      "healthCheck": {
        "http": {
          "endpoint": "httpendpoint",
          "path": "/",
          "scheme": "http"
        },
        "gracePeriodSeconds": 30,
        "intervalSeconds": 10,
        "maxConsecutiveFailures": 3,
        "timeoutSeconds": 5,
        "delaySeconds": 2
      }
    },
    {
      "name": "healthApp2",
      "exec": {
        "command": {
          "shell": ". /opt/mesosphere/environment.export && ./test_server.py $PORT_HTTPENDPOINT2"
        }
      },
      "resources": {
        "cpus": 0.1,
        "mem": 32,
        "disk": 0,
        "gpus": 0
      },
      "endpoints": [
        {
          "name": "httpendpoint2",
          "hostPort": 0,
          "protocol": [
            "http"
          ],
          "labels": {}
        }
      ],
      "image": {
        "kind": "docker",
        "id": "debian:jessie"
      },
      "healthCheck": {
        "http": {
          "endpoint": "httpendpoint2",
          "path": "/",
          "scheme": "http"
        },
        "gracePeriodSeconds": 30,
        "intervalSeconds": 10,
        "maxConsecutiveFailures": 3,
        "timeoutSeconds": 5,
        "delaySeconds": 2
      }
    }
  ],
  "networks": [
    {
      "name": null,
      "mode": "host",
      "labels": {}
    }
  ]
}
